{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "analyze": "cross-env ANALYZE=true npm run build",
    "type-check": "tsc --noEmit",
    "generate-static": "node scripts/generate-static-pages.js",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "optimize-static": "node scripts/optimize-static-pages.js",
    "build-static": "npm run generate-static && npm run generate-sitemap && npm run optimize-static",
    "export-static": "node scripts/build-static-site.js",
    "prepare-cloudlinux": "node scripts/prepare-cloudlinux-deployment.js"
  },
  "dependencies": {
    "cross-env": "^10.0.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
